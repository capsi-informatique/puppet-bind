{
  "name": "capsi-bind",
  "version": "3.0.0",
  "author": "CAPSI Informatique",
  "description": "Bind defined resource types",
  "summary": "Bind Module forked from Camptocamp",
  "license": "Apache-2.0",
  "source": "https://github.com/capsi-informatique/puppet-bind",
  "project_page": "https://github.com/capsi-informatique/puppet-bind",
  "issues_url": "https://github.com/capsi-informatique/puppet-bind/issues",
  "operatingsystem_support": [
    {
